{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": [
            "jquery-3.4.1.min.js",
            "bootstrap.min.js",
            "calculate.js"
          ],
          "css": [
            "bootstrap.min.css"
          ]
        }
      ],
    "browser_action":{
      "default_popup": "popup.html",
      "default_title": "Click here to see the grade report.",
      "default_icon": "book.png"
    },

    "permissions": [
      "storage",
      "tabs"
    ]
  }